// Should genertate: void @formatName(string*, string*, string*)
func formatName(string firstName, string lastName) (string) {
    return lastName + ", " + firstName
}

func fibonacci(int x) (int) {
    if x <= 1 {
        return 1
    } else {
        return fibonacci(x - 2) + fibonacci(x - 1)
    }
}

